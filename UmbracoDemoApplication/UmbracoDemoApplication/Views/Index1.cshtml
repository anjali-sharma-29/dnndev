@inherits Umbraco.Web.Mvc.UmbracoViewPage<UmbracoDemoApplication.Models.Index>
@using System.Web.Mvc.Html
@{
	Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>CMS</title>
    <link rel="stylesheet" href="/css/bootstrap.min.css">                       <!-- Bootstrap style, http://v4-alpha.getbootstrap.com/ -->
    <link rel="stylesheet" href="/css/templatemo-style.css">                    <!-- Templatemo style -->
</head>

   <body>
      
    <div class="container">
        <div class="row">
            <div class="col-lg-12">
                <h1>@Model.Header</h1>
                    @Model.BodyText
                    @Model.Content.GetPropertyValue("check")
                    <img src="@Model.Content.GetPropertyValue("image")">
            </div>
        </div>
</body>
</html>